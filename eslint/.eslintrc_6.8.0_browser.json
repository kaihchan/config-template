{
    "env": {
        "browser": true,
        "es6": false
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 5
    },
    "rules": {
        "no-undef": "off",
        "no-unused-vars": "off",
        "no-redeclare": "off",
        "no-mixed-spaces-and-tabs": "off"
    }
}
